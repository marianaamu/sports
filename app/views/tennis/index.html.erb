<h1>Listing tennis</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tennis.each do |tenni| %>
      <tr>
        <td><%= tenni.description %></td>
        <td><%= tenni.status %></td>
        <td><%= link_to 'Show', tenni %></td>
        <td><%= link_to 'Edit', edit_tenni_path(tenni) %></td>
        <td><%= link_to 'Destroy', tenni, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenni', new_tenni_path %>
